{
    "type": "service",
    "body": [
      {
        "type": "steedos-object-listview",
        "objectApiName": "${objectName}",
        "columnsTogglable": false,
        "showHeader": true,
        "id": "u:d29cef0d1007",
        "sideSchema": {
          "type": "service",
          "onEvent": {
            "@data.changed.instance_tasks": {
              "actions": [
                {
                  "actionType": "reload"
                }
              ]
            }
          },
          "body": [
            {
              "type": "input-tree",
              "name": "tree",
              "className": "instance-inbox-tree bg-white h-full w-full",
              "id": "u:9f3dd961ca12",
              "stacked": true,
              "multiple": false,
              "enableNodePath": false,
              "hideRoot": true,
              "showIcon": true,
              "initiallyOpen": false,
              "size": "md",
              "onEvent": {
                "change": {
                  "actions": [
                    {
                      "actionType": "setValue",
                      "componentId": "u:4dd57bf1a99a",
                      "args": {
                        "value": {
                          "additionalFilters": [
                            "${event.data.value.name}",
                            "=",
                            "${event.data.value.value}"
                          ]
                        }
                      },
                      "expression": "${event.data.value.level>=10}"
                    },
                    {
                      "args": {
                        "link": "${event.data.value.to}",
                        "blank": false
                      },
                      "actionType": "link",
                      "expression": "${event.data.value.level<10}"
                    }
                  ]
                }
              },
              "menuTpl": {
                "type": "wrapper",
                "className": "flex flex-row  p-0 m-0",
                "body": [
                  {
                    "type": "tpl",
                    "className": "flex-1 w-6/12",
                    "tpl": "${label}"
                  },
                  {
                    "type": "tpl",
                    "className": "-mx-11 ",
                    "tpl": "",
                    "badge": {
                      "offset": [
                        -13,
                        13
                      ],
                      "mode": "text",
                      "text": "${tag}"
                    }
                  }
                ]
              },
              "unfoldedLevel": 2,
              "source": "${options}"
            }
          ],
          "id": "u:aa4f4dc5f439",
          "api": {
            "method": "get",
            "url": "${context.rootUrl}/api/${appId}/workflow/nav",
            "headers": {
              "Authorization": "Bearer ${context.tenantId},${context.authToken}"
            }
          }
        },
        "showDisplayAs": true,
        "className": "h-full w-full bg-white",
        "formFactor": "LARGE"
      }
    ],
    "data": {
      "objectName": "instances"
    },
    "className": "h-full w-full p-0 m-0",
    "id": "u:4dd57bf1a99a",
    "onEvent": {
      "@data.changed.steedos_keyvalues": {
        "actions": [
          {
            "actionType": "broadcast",
            "args": {
              "eventName": "@data.changed.instance_tasks"
            },
            "data": {
              "objectName": "instance_tasks",
              "recordId": "reload"
            }
          }
        ]
      }
    }
  }